@import "/static/iconont/iconfont.wxss";

.navContainer{
  display: flex;
}

.navItem{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 20%;
}

.navItem .iconfont{
  width: 100rpx;
  height: 100rpx;
  background-color: rgb(240, 19, 19);
  text-align: center;
  line-height: 100rpx;
  border-radius: 50%;
  font-size: 50rpx;
  color: #fff;
  margin: 20rpx 0;
}

.navItem text{
  font-size: 24rpx;
}

